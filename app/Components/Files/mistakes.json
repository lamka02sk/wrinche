{

  "name":"mistakes",
  "active":true,
  "defaultElements":true,
  "header":"component_header",

  "prototype": {

    "mistakes": {
      "type":"boolean",

      "if": [
        {
          "equals":true,
          "prototype": {

            "mistakes_notify": {
              "type":"boolean",

              "if": [
                {
                  "equals":true,
                  "prototype": {

                    "mistakes_email": {
                      "type":"boolean"
                    }

                  }
                }
              ]
            }

          }
        }
      ]
    }

  },

  "template": [

    {
      "element":"label",
      "attributes": {
        "class":"label checkbox-click",
        "for":"component_mistakes",
        "data-locale":"COMPONENT_MISTAKES_ENABLE_LABEL"
      }
    },

    {
      "element":"input>",
      "label":"component_mistakes",
      "attributes": {
        "class":"input",
        "type":"checkbox",
        "name":"component_mistakes",
        "value":"component_mistakes"
      }
    },

    {
      "element":"label",
      "attributes": {
        "class":"label checkbox-click",
        "for":"component_mistakes_notify",
        "data-locale":"COMPONENT_MISTAKES_NOTIFY_LABEL"
      }
    },

    {
      "element":"input>",
      "label":"component_mistakes_notify",
      "attributes": {
        "class":"input",
        "type":"checkbox",
        "name":"component_mistakes_notify",
        "value":"component_mistakes_notify"
      }
    },

    {
      "element":"label",
      "attributes": {
        "class":"label checkbox-click",
        "for":"component_mistakes_email",
        "data-locale":"COMPONENT_MISTAKES_E-MAIL_LABEL"
      }
    },

    {
      "element":"input>",
      "label":"component_mistakes_email",
      "attributes": {
        "class":"input",
        "type":"checkbox",
        "name":"component_mistakes_email",
        "value":"component_mistakes_email"
      }
    }

  ]

}