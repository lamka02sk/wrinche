{

  "name":"note",
  "active":true,
  "defaultElements":true,

  "template": [

    {
      "element":"textarea",
      "label":"content-note",
      "attributes": {
        "class":"small-textarea",
        "name":"write_content_note",
        "data-placeholder":"WRITE_CONTENT_NOTE_PLACEHOLDER"
      }
    },

    {
      "element":"div",
      "attributes": {
        "class":"note-position"
      },
      "children": [

        {
          "element":"span",
          "attributes": {
            "class":"position-item left-position",
            "data-position":"0",
            "data-locale":"WRITE_CONTENT_POSITION_LEFT"
          }
        },

        {
          "element":"span",
          "attributes": {
            "class":"position-item right-position",
            "data-position":"1",
            "data-locale":"WRITE_CONTENT_POSITION_RIGHT"
          }
        },

        {
          "element":"span",
          "attributes": {
            "class":"position-item full-position",
            "data-position":"2",
            "data-locale":"WRITE_CONTENT_POSITION_FULL"
          }
        }

      ]
    }

  ]

}