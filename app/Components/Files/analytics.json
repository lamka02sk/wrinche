{

  "name":"analytics",
  "active":true,
  "defaultElements":false,

  "prototype": {

    "analytics": {
      "type":"boolean",

      "if": [
        {
          "equals":true,
          "prototype": {

            "analytics_details": {
              "type":"boolean"
            }

          }
        }
      ]

    }

  },

  "template": [

    {
      "element":"label",
      "attributes": {
        "class":"label checkbox-click",
        "for":"component_analytics",
        "data-locale":"COMPONENT_ANALYTICS_LABEL"
      }
    },

    {
      "element":"input>",
      "label":"component_analytics",
      "attributes": {
        "type":"checkbox",
        "class":"input",
        "value":"component_analytics",
        "name":"component_analytics"
      }
    },

    {
      "element":"div",
      "attributes": {
        "class":"input-box hide"
      },
      "children": [

        {
          "element":"label",
          "attributes": {
            "class":"label checkbox-click",
            "for":"component_analytics_details",
            "data-locale":"COMPONENT_ANALYTICS_DETAILS_LABEL"
          }
        },

        {
          "element":"input>",
          "label":"component_analytics_details",
          "attributes": {
            "type":"checkbox",
            "class":"input",
            "value":"component_analytics_details",
            "name":"component_analytics_details"
          }
        }

      ]
    }

  ]

}