{

  "name":"created_at",
  "active":true,
  "defaultElements":true,
  "header":"component_header",

  "prototype": {

    "created_at": {
      "type":"string",

      "validate": {
        "source":"validator",
        "method":"isPastTimestamp"
      }
    }

  },

  "template": [

    {
      "element":"label",
      "attributes": {
        "class":"label checkbox-click",
        "for":"component_created",
        "data-locale":"COMPONENT_CREATED_LABEL"
      }
    },

    {
      "element":"input>",
      "label":"component_created",
      "attributes": {
        "type":"checkbox",
        "class":"input",
        "value":"created",
        "name":"component_created"
      }
    },

    {
      "element":"template",
      "attributes": {
        "id":"template_component_created_custom"
      },
      "children": [

        {
          "element":"label",
          "attributes": {
            "class":"small-label",
            "for":"component_created_custom",
            "data-locale":"COMPONENT_CREATED_CUSTOM_LABEL"
          }
        },

        {
          "element":"input>",
          "label":"component_created_custom",
          "attributes": {
            "class":"input date-picker",
            "name":"component_created_custom",
            "data-placeholder":"COMPONENT_CREATED_CUSTOM_PLACEHOLDER"
          }
        }

      ]
    }

  ]

}