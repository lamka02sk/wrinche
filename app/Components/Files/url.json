{

  "name":"url",
  "active":true,
  "defaultElements":true,
  "header":"component_header",

  "prototype": {

    "url": {
      "type":"string",
      "length":"3,120",
      "validate": {
        "source":"validator",
        "method":"validateSimpleUrl"
      }
    }

  },

  "template": [

    {
      "element":"div",
      "attributes": {
        "class":"counter url-counter",
        "data-input":"input",
        "data-maxLength":"120"
      }
    },

    {
      "element":"input>",
      "label":"component_url",
      "attributes": {
        "class":"input",
        "type":"text",
        "data-placeholder":"COMPONENT_URL_PLACEHOLDER",
        "name":"component_url"
      }
    }

  ]

}