{

  "name":"wrinche",
  "description":"Modern, powerful and user friendly CMS.",
  "version":"1.0.0",
  "domain":"/",
  "charset":"UTF-8",

  "requirements": {
    "php-version":7.0
  },

  "default": {
    "theme":"light"
  },

  "env": {
    "debug":true
  },

  "locale": {
    "timezone":"UTC",
    "language":"en"
  },

  "paths": {
    "assets":"assets/",
    "styles":"styles/",
    "scripts":"scripts/",
    "fonts":"fonts/",
    "vendor":"vendor/"
  },

  "vendor": {
    "styles": {
      "selector":"selector.min.css"
    },
    "scripts": {
      "jquery":"jquery.min.js",
      "selector":"selector.min.js"
    }
  },

  "tracking": {
    "client":true,
    "stats":true,
    "location":true
  },

  "support": {
    "languages": {
      "en":"ENGLISH",
      "sk":"SLOVAK"
    },
    "themes": {
      "light":"LIGHT",
      "dark":"DARK"
    },
    "categories": {
      "0":"ART",
      "1":"ANIMALS",
      "2":"DESIGN",
      "3":"ELECTRONICS",
      "4":"HOLIDAYS",
      "5":"EDUCATION",
      "6":"BUSINESS",
      "7":"CARS",
      "8":"SPORT",
      "9":"FASHION",
      "10":"COMPUTERS",
      "11":"FOOD",
      "12":"SOCIETY",
      "13":"HOME",
      "14":"ENTERTAINMENT",
      "15":"ESTATE",
      "16":"MEDICAL",
      "17":"GARDEN"
    }
  },

  "installed":false

}