{
  "name": "wrinche",
  "description": "Modern, powerful and user friendly CMS.",
  "version": "0.0.2 alpha",
  "domain": "\/",
  "charset": "UTF-8",
  "requirements": {
    "php-version": 7,
    "mysql-version": 5.6
  },
  "default": {
    "theme": "light"
  },
  "env": {
    "debug": true,
    "maintenance": false
  },
  "locale": {
    "timezone": "UTC",
    "language": "en"
  },
  "paths": {
    "assets": "assets\/",
    "styles": "styles\/",
    "scripts": "scripts\/",
    "fonts": "fonts\/",
    "vendor": "vendor\/",
    "admin": ""
  },
  "vendor": {
    "styles": {
      "selector": "selector.min.css"
    },
    "scripts": {
      "jquery": "jquery.min.js",
      "selector": "selector.min.js",
      "translate": "translate.min.js",
      "theme": "theme.min.js"
    }
  },
  "tracking": {
    "client": true,
    "stats": true,
    "location": true,
    "performance": true,
    "errors": true,
    "operations": true
  },
  "support": {
    "languages": {
      "en": "ENGLISH",
      "sk": "SLOVAK"
    },
    "themes": {
      "light": "LIGHT",
      "dark": "DARK"
    },
    "categories": [
      "ART",
      "ANIMALS",
      "DESIGN",
      "ELECTRONICS",
      "HOLIDAYS",
      "EDUCATION",
      "BUSINESS",
      "CARS",
      "SPORT",
      "FASHION",
      "COMPUTERS",
      "FOOD",
      "SOCIETY",
      "HOME",
      "ENTERTAINMENT",
      "ESTATE",
      "MEDICAL",
      "GARDEN"
    ]
  },
  "installed": false,
  "administration": "",
  "installation_migrations": [
    "users",
    "tokens",
    "settings",
    "website"
  ]
}